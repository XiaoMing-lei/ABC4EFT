  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"scalaramp", "[", 
     RowBox[{"Num_", ",", "k_"}], "]"}], ":=", 
    RowBox[{
     RowBox[{"scalaramp", "[", 
      RowBox[{"Num", ",", "k"}], "]"}], "=", 
     RowBox[{"SSYT", "[", 
      RowBox[{
       RowBox[{"ConstantArray", "[", 
        RowBox[{"0", ",", "Num"}], "]"}], ",", "k", ",", 
       RowBox[{"OutMode", "\[Rule]", "\"\<amplitude\>\""}]}], "]"}]}]}], 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"CP", " ", "odd", " ", "combination", " ", "of", " ", 
     RowBox[{"momenta", " ", "--"}], " ", "building", " ", "blocks", " ", 
     "for", " ", "CP", " ", "odd", " ", 
     RowBox[{"amplitudes", "/", "operators"}]}], " ", "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"\[Epsilon]odd", "[", 
     RowBox[{"i_", ",", "j_", ",", "k_", ",", "l_"}], "]"}], ":=", 
    RowBox[{
     RowBox[{
      RowBox[{"ab", "[", 
       RowBox[{"i", ",", "j"}], "]"}], 
      RowBox[{"sb", "[", 
       RowBox[{"j", ",", "k"}], "]"}], 
      RowBox[{"ab", "[", 
       RowBox[{"k", ",", "l"}], "]"}], 
      RowBox[{"sb", "[", 
       RowBox[{"l", ",", "i"}], "]"}]}], "-", 
     RowBox[{
      RowBox[{"sb", "[", 
       RowBox[{"i", ",", "j"}], "]"}], 
      RowBox[{"ab", "[", 
       RowBox[{"j", ",", "k"}], "]"}], 
      RowBox[{"sb", "[", 
       RowBox[{"k", ",", "l"}], "]"}], 
      RowBox[{"ab", "[", 
       RowBox[{"l", ",", "i"}], "]"}]}]}]}], " ", 
   RowBox[{"(*", " ", 
    RowBox[{"4", 
     SubscriptBox["i\[Epsilon]", "\[Mu]\[Nu]\[Rho]\[Sigma]"], 
     SuperscriptBox[
      SubscriptBox["p", "i"], "\[Mu]"], 
     SuperscriptBox[
      SubscriptBox["p", "j"], "\[Vee]"], 
     SuperscriptBox[
      SubscriptBox["p", "k"], "\[Rho]"], 
     SuperscriptBox[
      SubscriptBox["p", "l"], "\[Sigma]"]}], " ", "*)"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{
       "generate", " ", "complete", " ", "basis", " ", "of", " ", "initial", 
        " ", "monomial", " ", "soft", " ", "amp", " ", "with", " ", "given", 
        " ", 
        RowBox[{"parity", "."}]}], "\[IndentingNewLine]", "<", "BasisReduce", 
       ">", " ", 
       RowBox[{
        RowBox[{"reduce", " ", "the", " ", "amp", " ", "to", " ", "y"}], "-", 
        RowBox[{
        "basis", " ", "and", " ", "store", " ", "the", " ", 
         "coordinates"}]}]}], ";"}], "\[IndentingNewLine]", "  ", "<", 
     "FindIndependentBasis", ">", " ", 
     RowBox[{
     "find", " ", "independent", " ", "soft", " ", "amp", " ", "and", " ", 
      "store", " ", "the", " ", "positions", " ", "in", " ", "result"}]}], 
    " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{"Clear", "[", "SoftComb", "]"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"Options", "[", "SoftComb", "]"}], "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Parity", "\[Rule]", "\"\<even\>\""}], ",", 
       RowBox[{"BasisReduce", "\[Rule]", "True"}], ",", 
       RowBox[{"FindIndependentBasis", "\[Rule]", "True"}], ",", 
       RowBox[{"ShowProgress", "\[Rule]", "False"}]}], "}"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"SoftComb", "[", 
     RowBox[{"N_", ",", "k_", ",", 
      RowBox[{"OptionsPattern", "[", "]"}]}], "]"}], ":=", 
    RowBox[{
     RowBox[{"SoftComb", "[", 
      RowBox[{"N", ",", "k", ",", 
       RowBox[{"Parity", "\[Rule]", 
        RowBox[{"OptionValue", "[", "Parity", "]"}]}]}], "]"}], "=", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"result", ",", "len", ",", 
         RowBox[{"iter", "=", "0"}], ",", 
         RowBox[{"reduced", "=", 
          RowBox[{"{", "}"}]}], ",", "pos"}], "}"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"result", "=", 
         RowBox[{"<|", 
          RowBox[{"\"\<initial\>\"", "\[Rule]", 
           RowBox[{"Switch", "[", 
            RowBox[{
             RowBox[{"OptionValue", "[", "Parity", "]"}], ",", "\"\<even\>\"",
              ",", 
             RowBox[{"SoftEvenComb", "[", 
              RowBox[{"N", ",", 
               RowBox[{"k", "/", "2"}]}], "]"}], ",", "\"\<odd\>\"", ",", 
             RowBox[{"SoftOddComb", "[", 
              RowBox[{"N", ",", 
               RowBox[{"k", "/", "2"}]}], "]"}]}], "]"}]}], "|>"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"OptionValue", "[", "BasisReduce", "]"}], ",", 
          RowBox[{
           RowBox[{"len", "=", 
            RowBox[{"Length", "@", 
             RowBox[{"result", "[", "\"\<initial\>\"", "]"}]}]}], ";", 
           RowBox[{"PrintTemporary", "[", 
            RowBox[{"\"\<  reducing ... \>\"", ",", 
