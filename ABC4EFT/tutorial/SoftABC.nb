Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"Y", "-", 
       RowBox[{"basis", " ", "for", " ", "scalar", " ", 
        RowBox[{"amplitudes", ":", " ", 
