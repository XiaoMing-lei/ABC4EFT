           RowBox[{"Do", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"iter", "++"}], ";", "\[IndentingNewLine]", 
              RowBox[{"AppendTo", "[", 
               RowBox[{"reduced", ",", 
                RowBox[{"AmpReduce", "[", 
                 RowBox[{
                  RowBox[{"amp", "/.", 
                   RowBox[{"\[Epsilon]p", "\[Rule]", "\[Epsilon]odd"}]}], ",",
                   "N"}], "]"}]}], "]"}]}], ",", 
             RowBox[{"{", 
              RowBox[{"amp", ",", 
               RowBox[{"result", "[", "\"\<initial\>\"", "]"}]}], "}"}]}], 
